function catAndMouse(x, y, z) {
    let distanceA = 0;
    let distanceB = 0;
    
    if (x < z){
        distanceA = z - x;
    }else{
        distanceA = x - z;
    }
    if (y < z){
        distanceB = z - y;
    }else{
        distanceB = y - z;
    }
    if(distanceA < distanceB){
        return ("Cat A")
        
    }else if(distanceB < distanceA){
        return ("Cat B")
        
    }else{
        return ("Mouse C")
    }
}
