function kangaroo(x1, v1, x2, v2) {
    let differenceX = 0;
    let differenceV = 0;
    
    if(x1 > x2){
        differenceX = x1 - x2;
    }else{
        differenceX = x2 - x1;
    }
     
    if(v1 > v2){
        differenceV = v1 - v2;
    }else{
        differenceV = v2 - v1;
    }
    if (x1 > x2 && v1 > v2 || x2 > x1 && v2 > v1) {
        return ("NO")
    }
    if(differenceX % differenceV == 0){
        return("YES")
    }else{
        return("NO")
    }
    
}
